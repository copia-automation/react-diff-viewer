{
  "name": "@copia-automation/react-diff-viewer",
  "version": "4.0.0-4",
  "private": false,
  "description": "A simple and beautiful text diff viewer component made with diff and React",
  "keywords": [
    "review",
    "code-review",
    "diff",
    "diff-viewer",
    "github",
    "react",
    "react-component",
    "ui"
  ],
  "files": [
    "lib/*.js",
    "lib/*.d.ts",
    "lib/**/*.js",
    "lib/**/*.d.ts"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "license": "MIT",
  "author": "Copia Automation (originally Pranesh Ravi <praneshpranesh@gmail.com>)",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "build": "yarn build:worker && tsc --outDir lib/",
    "build:examples": "webpack",
    "build:watch": "tsc --outDir lib/ -w",
    "build:worker": "esbuild src/getLinesToRenderWorker.ts --bundle --outfile=lib/getLinesToRender.worker.js",
    "lint": "yarn prettier && eslint",
    "lint:fix": "yarn prettier --write && yarn eslint --fix",
    "prettier": "prettier --check \"./**/*.{ts,tsx,js,json,jsx,scss,html,yaml,yml}\"",
    "start:examples": "webpack-dev-server --open",
    "test": "vitest"
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "classnames": "^2.5.1",
    "diff": "^8.0.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-virtuoso": "^4.14.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^12.0.12",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/web-worker": "^3.2.4",
    "css-loader": "^7.1.2",
    "esbuild": "^0.25.9",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "favicons": "^7.2.0",
    "favicons-webpack-plugin": "^6.0.1",
    "file-loader": "^6.2.0",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.4",
    "mocha": "^11.7.1",
    "prettier": "^3.6.2",
    "raw-loader": "^4.0.2",
    "react-dom": "^19.1.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "spy": "^1.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "webpack": "^5.101.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
