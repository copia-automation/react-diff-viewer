{
	"name": "@copia-automation/react-diff-viewer",
	"version": "3.1.3",
	"private": false,
	"description": "A simple and beautiful text diff viewer component made with diff and React",
	"keywords": [
		"review",
		"code-review",
		"diff",
		"diff-viewer",
		"github",
		"react",
		"react-component",
		"ui"
	],
	"files": [
		"lib/*.js",
		"lib/*.d.ts"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"license": "MIT",
	"author": "Pranesh Ravi<praneshpranesh@gmail.com>",
	"main": "lib/index",
	"typings": "lib/index",
	"scripts": {
		"build": "tsc --outDir lib/",
		"build:examples": "webpack",
		"build:watch": "tsc --outDir lib/ -w",
		"publish:examples": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist -r $GITHUB_REPO_URL",
		"publish:examples:local": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist",
		"start:examples": "webpack-dev-server --open",
		"test": "mocha --require ts-node/register --require enzyme.ts ./test/**",
		"test:watch": "mocha --require ts-node/register --require enzyme.ts --watch-extensions ts,tsx --watch ./test/**"
	},
	"dependencies": {
		"@emotion/css": "^11.13.5",
		"@emotion/react": "^11.14.0",
		"classnames": "^2.2.6",
		"diff": "^4.0.1",
		"memoize-one": "^5.0.4",
		"prop-types": "^15.6.2"
	},
	"devDependencies": {
		"@types/classnames": "^2.2.6",
		"@types/diff": "^4.0.2",
		"@types/enzyme": "^3.1.14",
		"@types/enzyme-adapter-react-16": "^1.0.3",
		"@types/expect": "^1.20.3",
		"@types/memoize-one": "^4.1.1",
		"@types/mocha": "^10.0.10",
		"@types/node": "^12.0.12",
		"@types/react": "^16.4.14",
		"@types/react-dom": "^16.0.8",
		"@types/webpack": "^5.28.5",
		"@typescript-eslint/eslint-plugin": "^8.25.0",
		"@typescript-eslint/parser": "^8.25.0",
		"css-loader": "^7.1.2",
		"enzyme": "^3.7.0",
		"enzyme-adapter-react-16": "^1.6.0",
		"eslint": "^9.21.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.1.0",
		"expect": "^24.8.0",
		"favicons": "^7.2.0",
		"favicons-webpack-plugin": "^6.0.1",
		"file-loader": "^6.2.0",
		"gh-pages": "^5.0.0",
		"html-webpack-plugin": "^5.6.3",
		"mini-css-extract-plugin": "^2.9.2",
		"mocha": "^11.1.0",
		"raw-loader": "^4.0.2",
		"react": "^16.5.2",
		"react-dom": "^16.5.2",
		"sass": "^1.83.4",
		"sass-loader": "^16.0.5",
		"spy": "^1.0.0",
		"ts-loader": "^9.5.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.3",
		"webpack": "^5.98.0",
		"webpack-cli": "^6.0.1",
		"webpack-dev-server": "^5.2.0"
	},
	"peerDependencies": {
		"react": "^15.3.0 || ^16.0.0",
		"react-dom": "^15.3.0 || ^16.0.0"
	},
	"engines": {
		"node": ">= 8"
	}
}
