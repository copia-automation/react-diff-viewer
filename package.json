{
	"name": "@copia-automation/react-diff-viewer",
	"version": "3.1.5-6",
	"private": false,
	"description": "A simple and beautiful text diff viewer component made with diff and React",
	"keywords": [
		"review",
		"code-review",
		"diff",
		"diff-viewer",
		"github",
		"react",
		"react-component",
		"ui"
	],
	"files": [
		"lib/*.js",
		"lib/*.d.ts"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"license": "MIT",
	"author": "Pranesh Ravi<praneshpranesh@gmail.com>",
	"main": "lib/index",
	"typings": "lib/index",
	"scripts": {
		"build": "tsc --outDir lib/",
		"build:examples": "webpack",
		"build:watch": "tsc --outDir lib/ -w",
		"lint": "yarn eslint",
		"lint:fix": "yarn eslint --fix",
		"publish:examples": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist -r $GITHUB_REPO_URL",
		"publish:examples:local": "NODE_ENV=production yarn build:examples && gh-pages -d examples/dist",
		"start:examples": "webpack-dev-server --open"
	},
	"dependencies": {
		"@emotion/css": "^11.13.5",
		"@emotion/react": "^11.14.0",
		"classnames": "^2.5.1",
		"diff": "^8.0.2",
		"memoize-one": "^6.0.0",
		"prop-types": "^15.8.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.33.0",
		"@types/enzyme": "^3.10.19",
		"@types/mocha": "^10.0.10",
		"@types/node": "^12.0.12",
		"@types/react": "^19.1.10",
		"@types/react-dom": "^19.1.7",
		"@types/webpack": "^5.28.5",
		"@typescript-eslint/eslint-plugin": "^8.39.1",
		"@typescript-eslint/parser": "^8.39.1",
		"css-loader": "^7.1.2",
		"eslint": "^9.33.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^5.2.0",
		"expect": "^30.0.5",
		"favicons": "^7.2.0",
		"favicons-webpack-plugin": "^6.0.1",
		"file-loader": "^6.2.0",
		"gh-pages": "^6.3.0",
		"globals": "^16.3.0",
		"html-webpack-plugin": "^5.6.3",
		"mini-css-extract-plugin": "^2.9.4",
		"mocha": "^11.7.1",
		"raw-loader": "^4.0.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"sass": "^1.90.0",
		"sass-loader": "^16.0.5",
		"spy": "^1.0.0",
